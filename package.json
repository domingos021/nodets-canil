{
  "name": "nodets-canil",
  "version": "1.0.0",
  "description": "back-end do projeto canil",
  "homepage": "https://github.com/domingos021/nodets-canil#readme",
  "bugs": {
    "url": "https://github.com/domingos021/nodets-canil/issues"
  },
  "engines": {
    "node": "^22.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domingos021/nodets-canil.git"
  },
  "license": "ISC",
  "author": "Domingos Dinis Jose Jovete",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts -e ts,json,mustache,css",
    "build": "tsc && npm run copy",
    "copy": "copyfiles -u 1 src/**/*.mustache public/**/* dist/",
    "start": "node dist/server.js",
    "watch:dev": "concurrently \"tsc --watch\" \"nodemon dist/server.js\""
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/mustache-express": "^1.2.5",
    "@types/node": "^22.0.0",
    "concurrently": "^8.0.0",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "mustache-express": "^1.3.2"
  }
}
